<?xml version="1.0" encoding="ISO-8859-1"?>
<paramInfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="https://seadas.gsfc.nasa.gov/software/schemas/ParamInfo-1.0.xsd">
    <programMetaData>
        <hasParFile>true</hasParFile>
        <parFileOptionName>par</parFileOptionName>
        <primaryOptions>
            <primaryOption>ifile</primaryOption>
            <primaryOption>ofile</primaryOption>
        </primaryOptions>
    </programMetaData>
    <options>
        <option type="help">
            <name>-help</name>
            <value>false</value>
            <type>bool</type>
            <default>false</default>
            <description>print usage information</description>
            <source>default</source>
            <alias>-h</alias>
            <alias>--help</alias>
        </option>
        <option type="help">
            <name>-version</name>
            <value>false</value>
            <type>bool</type>
            <default>false</default>
            <description>print the version
                information
            </description>
            <source>default</source>
            <alias>--version</alias>
        </option>
        <option type="help">
            <name>-dump_options</name>
            <value>false</value>
            <type>bool</type>
            <default>false</default>
            <description>print
                information about each option
            </description>
            <source>default</source>
            <alias>--dump_options</alias>
        </option>
        <option type="help">
            <name>-dump_options_paramfile</name>
            <value/>
            <type>string</type>
            <description>print
                information about each option to paramfile
            </description>
            <source>default</source>
            <alias>--dump_options_paramfile</alias>
        </option>
        <option type="help">
            <name>-dump_options_xmlfile</name>
            <value>l2brsgen.xml</value>
            <type>string</type>
            <description>print
                information about each option to XML file
            </description>
            <source>command line</source>
            <alias>--dump_options_xmlfile</alias>
        </option>
        <option type="ifile">
            <name>ifile</name>
            <value/>
            <description>input L2 file name</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="ofile">
            <name>ofile</name>
            <value>output</value>
            <default>output</default>
            <description>output filename</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="string">
            <name>prod</name>
            <value></value>
            <type>string</type>
            <default>chlor_a</default>
            <description>product name</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="int">
            <name>quality</name>
            <value></value>
            <default>999</default>
            <description>highest quality value acceptable</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="string">
            <name>rflag</name>
            <value></value>
            <type>string</type>
            <default>ORIGINAL</default>
            <description>replacement flag</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="string">
            <name>flaguse</name>
            <value/>
            <type>string</type>
            <description>Flags used to mask data</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="string">
            <name>chl_flags</name>
            <value></value>
            <type>string</type>
            <default>ATMFAIL,HILT,STRAYLIGHT,CLDICE,LOWLW,CHLWARN,CHLFAIL,NAVWARN,MAXAERITER,NAVFAIL,FILTER,HIGLINT
            </default>
            <description>Flags used to mask data for chl product if flaguse not set</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="string">
            <name>sst_flags</name>
            <value></value>
            <type>string</type>
            <default>SSTFAIL</default>
            <description>Flags used to mask data for sst product if flaguse not set</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="int">
            <name>spixl</name>
            <value></value>
            <type>int</type>
            <default>1</default>
            <description>start pixel number</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="int">
            <name>epixl</name>
            <value></value>
            <type>int</type>
            <default>-1</default>
            <description>end pixel number (-1=the last pixel)</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="int">
            <name>dpixl</name>
            <value></value>
            <type>int</type>
            <default>1</default>
            <description>pixel subsampling interval</description>
            <source>/Users/Shared/ocssw/run/data/common/l2brsgen_defaults.par</source>
            <usedAs>option</usedAs>
        </option>
        <option type="int">
            <name>sline</name>
            <value></value>
            <type>int</type>
            <default>1</default>
            <description>start line number</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="int">
            <name>eline</name>
            <value></value>
            <type>int</type>
            <default>-1</default>
            <description>end line number (-1=the last line)</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="int">
            <name>dline</name>
            <value></value>
            <type>int</type>
            <default>1</default>
            <description>line subsampling interval</description>
            <source>/Users/Shared/ocssw/run/data/common/l2brsgen_defaults.par</source>
            <usedAs>option</usedAs>
        </option>
        <option type="boolean">
            <name>apply_pal</name>
            <value></value>
            <type>bool</type>
            <default>false</default>
            <description>apply color palette, false = grayscale</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="string">
            <name>palfile</name>
            <value></value>
            <type>string</type>
            <default>default</default>
            <description>palette filename. "default" means the
                palette is chosen using the product table file
            </description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="string">
            <name>palette_dir</name>
            <value></value>
            <type>string</type>
            <default>$OCDATAROOT/common/palette</default>
            <description>directory
                containing the palette files
            </description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="ifile">
            <name>product_table</name>
            <value></value>
            <type>string</type>
            <default>$OCDATAROOT/common/l2brsgen_product_table.dat</default>
            <description>product table</description>
            <source>default</source>
            <usedAs>option</usedAs>
        </option>
        <option type="float">
            <name>datamin</name>
            <value></value>
            <type>float</type>
            <default>0.0</default>
            <description>minimum value for data scaling
                (default: see product_table)
            </description>
            <source>default</source>
            <validValue>
                <value>0.0</value>
                <description>(default: see product_table)</description>
            </validValue>
            <usedAs>option</usedAs>
        </option>
        <option type="float">
            <name>datamax</name>
            <value></value>
            <type>float</type>
            <default>0.0</default>
            <description>maximum value for data scaling
                (default: see product_table)
            </description>
            <source>default</source>
            <validValue>
                <value>0.0</value>
                <description>(default: see product_table)</description>
            </validValue>
            <usedAs>option</usedAs>
        </option>
        <option type="int">
            <name>stype</name>
            <value></value>
            <type>int</type>
            <default>0</default>
            <description>scaling type: 1=LINEAR; 2=LOG;
                (default: see product_table)
            </description>
            <source>default</source>
            <validValue>
                <value>1</value>
                <description>LINEAR</description>
            </validValue>
            <validValue>
                <value>2</value>
                <description>LOG</description>
            </validValue>
            <usedAs>option</usedAs>
        </option>
        <option type="int">
            <name>oformat</name>
            <value>HDF4</value>
            <type>int</type>
            <default>HDF4</default>
            <description>format of the output file
                0: HDF browse file
                1: PPM color or PGM grayscale image file
                2: PNG color or grayscale image file
            </description>
            <source>default</source>
            <validValue>
                <value>HDF4</value>
                <description>HDF browse file</description>
            </validValue>
            <validValue>
                <value>PNG</value>
                <description>PNG color or grayscale image file</description>
            </validValue>
            <validValue>
                <value>PPM</value>
                <description>PPM color or PGM grayscale image file</description>
            </validValue>
            <usedAs>option</usedAs>
        </option>
    </options>
</paramInfo>
